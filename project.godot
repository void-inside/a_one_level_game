config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="a one level game"
run/main_scene="res://scenes/screens/boot.tscn"
run/low_processor_mode=true
boot_splash/bg_color=Color( 0.203922, 0.0313726, 0.0313726, 1 )
config/icon="res://icon.png"

[autoload]

GLOBAL="*res://scenes/_GLOBAL.tscn"

[display]

window/size/width=576
window/size/height=320
window/size/test_width=1152
window/size/test_height=640
window/stretch/mode="2d"
window/stretch/aspect="keep"

[rendering]

quality/driver/driver_name="GLES2"
quality/2d/use_pixel_snap=true
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
